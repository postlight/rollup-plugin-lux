{
  "name": "rollup-plugin-lux",
  "version": "3.0.0",
  "description": "A Rollup plugin for bundling Lux applications.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rollup -c",
    "clean": "shx rm -rf coverage dist junit.xml",
    "codecov": "curl -s https://codecov.io/bash | bash",
    "flow": "flow check",
    "install:types": "shx rm -rf flow-typed && flow-typed install",
    "lint": "remark . && eslint .",
    "test": "jest --coverage"
  },
  "author": "Zachary Golba",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.22.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "eslint": "3.14.1",
    "eslint-config-airbnb-base": "11.0.1",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-import": "2.2.0",
    "flow-bin": "0.38.0",
    "flow-typed": "2.0.0",
    "fs-promise": "1.0.0",
    "jest": "18.1.0",
    "jest-junit": "1.0.4",
    "remark-cli": "2.1.0",
    "remark-preset-lint-recommended": "1.0.0",
    "rollup": "0.41.4",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-node-resolve": "2.0.0",
    "shx": "0.2.2"
  },
  "dependencies": {
    "acorn": "4.0.5",
    "eol": "0.5.1",
    "magic-string": "0.19.0"
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-junit"
  }
}
